#both the following solutions work
# import re
for i in range(int(input())):
    p=input()
    try:
        re.compile(p)
        print('True')
    except re.error:
        print('False')

 #input
 1

.*+
#output
True

#another try of the above # it works
import re
n=int(input())
for i in range(n):
    myPattern=input()
    try:
        print(bool(re.compile(myPattern)))
    except:
        print (False)
        
 #input
 1

.*+
#output
False
 
