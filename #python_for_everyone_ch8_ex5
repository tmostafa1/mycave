my_text=open('F:\\OneDriveGSU\\OneDrive - Georgia State University\\Python_practice\\test3.txt')
count=0
for line in my_text:
    if line.startswith('From '):
        my_line=line.split()
        count+=1
        print(my_line[1])
print('there are {} lines that start with From'.format(count))

