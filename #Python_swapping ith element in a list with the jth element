#swapping ith element in a list with the jth element
#this code worked
def swap_list_element(lis, i, j):
    for n in range(0, len(lis)):
        if lis[n]==i:
            lis[n]=j
        elif lis[n]==j:
            lis[n]=i
        
    return lis
lis=[1, 3, 4, 5, 6, 4, 3]
i=3
j=6
print(swap_list_element(lis, i, j))

#output
[1, 6, 4, 5, 3, 4, 6]
