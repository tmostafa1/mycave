#write a code to sort a list of numbers
def sort_a_list(lis):    
    for i in range(len(lis)-1):
        if lis[i]<lis[i+1]:
            lis[i+1]=lis[i]
        elif lis[i]>lis[i+1]:
            lis[i]==lis[i+1]
    return lis[i+1]
    
lis=[56, 78, 98, 34, 23, 89, 56]     
print(sort_a_list(lis))
    
#output
23
